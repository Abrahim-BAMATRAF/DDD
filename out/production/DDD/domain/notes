	@Override
	public boolean equals(Object obj) {
		if(obj instanceof Reference){
			Reference tmp = (Reference) obj;
			return this.ref.getRef().equals(tmp.ref.getRef());
			
		}else{
			return false;
		}
	}
	
	
	
